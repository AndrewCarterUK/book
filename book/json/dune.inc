(alias
 (name runtest)
 (deps
  (:x README.md)
  (package core)
  (package mdx)
  (package ppx_jane)
  (package yojson)
  (:y13 examples/book.json)
  (:y12 examples/github.atd)
  (:y11 examples/github_j_excerpt.mli)
  (:y10 examples/github_org_info/dune)
  (:y9 examples/github_org_info/github_org.atd)
  (:y8 examples/github_org_info/github_org_info.ml)
  (:y7 examples/list_excerpt.mli)
  (:y6 examples/parse_book/dune)
  (:y5 examples/parse_book/parse_book.ml)
  (:y4 examples/read_json/dune)
  (:y3 examples/read_json/read_json.ml)
  (:y2 examples/yojson_basic.mli)
  (:y1 examples/yojson_basic_simple.mli)
  (:y0 examples/yojson_safe.mli)
  (source_tree examples)
  (source_tree examples/github_org_info)
  (source_tree examples/parse_book)
  (source_tree read_json)
  prelude.ml)
 (locks ../../global-lock)
 (action
  (progn
   (run ocaml-mdx test --prelude=prelude.ml %{x})
   (diff? %{x} %{x}.corrected)
   (diff? %{y13} %{y13}.corrected)
   (diff? %{y12} %{y12}.corrected)
   (diff? %{y11} %{y11}.corrected)
   (diff? %{y10} %{y10}.corrected)
   (diff? %{y9} %{y9}.corrected)
   (diff? %{y8} %{y8}.corrected)
   (diff? %{y7} %{y7}.corrected)
   (diff? %{y6} %{y6}.corrected)
   (diff? %{y5} %{y5}.corrected)
   (diff? %{y4} %{y4}.corrected)
   (diff? %{y3} %{y3}.corrected)
   (diff? %{y2} %{y2}.corrected)
   (diff? %{y1} %{y1}.corrected)
   (diff? %{y0} %{y0}.corrected))))

