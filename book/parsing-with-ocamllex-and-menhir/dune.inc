 (name runtest)
 (deps
  (:x README.md)
  (package core)
  (package mdx)
  (package ppx_jane)
  (:y12 examples/parsing-test/test.ml)
  (:y11 examples/parsing-test/test1.json)
  (:y10 examples/parsing/dune)
  (:y9 examples/parsing/json.ml)
