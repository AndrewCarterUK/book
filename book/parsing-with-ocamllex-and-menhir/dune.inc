  (:y0 examples/parsing/tokens.ml)
  (source_tree examples/parsing)
  (source_tree examples/parsing-test)
  prelude.ml)
 (locks ../../global-lock)
 (action
  (progn
   (run ocaml-mdx test --prelude=prelude.ml --direction=to-md %{x})
   (diff? %{x} %{x}.corrected)
   (diff? %{y12} %{y12}.corrected)
   (diff? %{y11} %{y11}.corrected)
